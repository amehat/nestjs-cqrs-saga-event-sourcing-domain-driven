{
  "name": "nestjs-cqrs-saga",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "export NODE_ENV=dev && concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cli": "ts-node -r tsconfig-paths/register src/core/console/bin/cli.ts",
    "cli:dev": "ts-node -r tsconfig-paths/register src/core/eventstore/cli/cli.ts",
    "feature": "./node_modules/.bin/cucumber-js -p default"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^1.15.0",
    "@nestjs-plus/rabbitmq": "^1.4.4",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/common": "^9.0.3",
    "@nestjs/core": "^9.0.3",
    "@nestjs/cqrs": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/typeorm": "^9.0.1",
    "@types/yargs": "^15.0.4",
    "amqplib": "^0.5.5",
    "chalk": "^4.0.0",
    "colors": "^1.4.0",
    "commander": "4.1.1",
    "cucumber": "^6.0.5",
    "cucumber-pretty": "^6.0.0",
    "cucumber-tsflow": "^3.2.0",
    "mongodb": "^3.5.5",
    "mosca": "^0.6.0",
    "mysql": "^2.18.1",
    "nestjs-console": "^8.0.0",
    "nestjs-redis": "^1.2.7",
    "node-emoji": "^1.10.0",
    "node-gyp": "^6.1.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.3.9",
    "@types/cucumber": "^6.0.1",
    "@types/express": "^4.17.4",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "concurrently": "^7.6.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.21",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "^29.0.5",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.7.2",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
